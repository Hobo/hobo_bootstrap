<!--
Adds bootstrap styling to `<live-search>`
-->
<extend tag="live-search">
  <old-live-search>
    <label: replace/>
    <form: class="form-search"/>
    <input: placeholder="Search" class="search-query"/>
    <after-input:>
      <i class="icon-search"></i>
    </after-input:>
  </old-live-search>
</extend>
